REACT_APP_TEST_VAR=testValueFromEnv

# Empty = same-origin requests (localkynews.com/api/*).
# In local dev the Vite proxy in vite.config.js routes /api/* to the local
# wrangler dev server at 127.0.0.1:8787 so the empty value works everywhere.
VITE_API_BASE_URL=
REACT_APP_API_BASE_URL=

